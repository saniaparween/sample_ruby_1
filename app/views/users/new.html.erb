<%= @messageFromControllerUserNotExist %>

<%= form_with scope: :user, url: users_path, local: true do |form| %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.text_area :password %>
  </p>

  <p>
    <%= form.submit %>
  </p>
 <%= link_to 'login', users_login_path %>
<% end %>
